</Layout>
<style>
    .container{
        display: grid;
		justify-content: center;
		gap: 10px;
		max-width: 800px;     

    }
    .title{
        display: flex;
        flex-direction: row;
        align-items: center;
        justify-content: space-between;
        .Icons{
            position: relative;
            flex-direction: row;            
            width: auto;
            height: 2rem;
            gap: 20px;            
            padding: 20px;

         }
    }
    .content{
        display: grid;
		justify-content: center;
		gap: 10px;	
        grid-template-columns: 1fr 1fr 1fr;
        grid-template-areas: 
            "a1 a1 a1"
            "a2 a2 a3"
            "a2 a2 a4"
            "a2 a2 a5"
            "a2 a2 a5"
            "a6 a6 a5"
            "a6 a6 a5"
            "a6 a6 a7"
            "a6 a6 a7"
            "a8 a8 a7"  
        ;
        
        }
		.a1{
            grid-area: a1;
        }
        .a2{
            grid-area: a2;
        }
        .a3{
            grid-area: a3;                        
            padding: 20px;
            background-color: #004AAB;
            color: rgb(236, 231, 255);
            border-radius: 10px;
            p{
                line-height: 1.5;
            }
        }
        .a4{
            grid-area: a4;
        }
        .a5{
            grid-area: a5;
        }
        .a6{
            grid-area: a6;
        }
        .a7{
            grid-area: a7;
        }
        .a8{
            grid-area: a8;
        }
        

        
    .img{
        padding: 0px;
        img {
			width: 100%;
			height: 100%;
			object-fit: cover;
			border-radius: 10px;			
		}
    }

/*-----------------------Query----------------------------- */

    @media (width < 1000px){
        .container{
            width: 100%;
        }
    }
    @media (width < 800px){
        .title{
            .Icons{
                height: 1.8rem;
                gap: 15px;            
                padding: 15px;
            }
        }
        .a3{
                                  
            padding: 10px;
            background-color: #004AAB;
            color: rgb(236, 231, 255);
            border-radius: 10px;
            p{
                line-height: 1.3;
            }
        }
        
    }
    @media (width < 600px){
        .title{
            .Icons{
                height: 1rem;
                gap: 10px;            
                padding: 10px;
            }
        }
        
    }
    
    
</style>